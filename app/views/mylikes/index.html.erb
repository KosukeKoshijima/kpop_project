<div class="page-header">
  <h1>Favorites</h1>
</div>
<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
      </tr>
      <% @user.liked_videos.each do |video| %>
        <dd><a href="/videos/<%= video.id %>"><img src="<%=video.image%>"></a></dd>
        <dd><%= video.caption %></dd>
      <% end %>
    </table>
  </div>
</div>
